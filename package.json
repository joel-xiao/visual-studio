{
  "name": "visual-studio",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "tauri:build": "vue-tsc --noEmit && vite build --outDir dist-tauri",
    "preview": "vite preview",
    "tauri": "tauri",
    "es:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue,.html",
    "es:lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.html",
    "log": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-option.js",
    "prepare": "husky install"
  },
  "changelog": {
    "bugsUrl": "https://github.com/huoyou/vue_cli3/issues/",
    "emojis": true,
    "authorName": true,
    "authorEmail": true
  },
  "lint-staged": {},
  "dependencies": {
    "@tauri-apps/api": "^1.5.2",
    "axios": "^1.6.7",
    "naive-ui": "^2.37.3",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "pug": "^3.0.2",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.70.0",
    "vfonts": "^0.0.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^7.0.14",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "typescript": "^5.0.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.5"
  },
  "optionalDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2"
  }
}
